.page{
  display: flex;
  flex-direction: column;
  background: #F8F8F8;
  padding: 30rpx 10rpx;
  color: #000;
  font-family: "微软雅黑";
}
.content
{
  text-align: left;
  display: block;
  background-color: #fff;
  margin:0rpx 20rpx; 
  border-radius: 10rpx;
  font-size: 34rpx;
  padding: 20rpx;
}

.content-title
{
  display: inline-block;
  color:#1DA27F;
  font-weight: bold;
  font-size: 40rpx;
  width: 180rpx;
  position: relative;
  top: -20rpx;
}

.content-middle{
  display: inline-block;
  line-height: 1.5;
  margin-left: 20rpx;
}

.content-rank
{
  display: inline-block;
  font-weight: bold;
}

.content-time{
  font-size: 30rpx;
}

.content-btn{
  float: right;
  position: relative;
  top: -10rpx;
}

.break{
  height: 20rpx;
}